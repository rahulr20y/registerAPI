 
-- table for express API
 
CREATE TABLE users (
 user_id INT PRIMARY KEY,
 first_name VARCHAR(40),
 last_name VARCHAR(40),
 birth_date DATE,
 sex VARCHAR(1),
 email VARCHAR(40),
 pass VARCHAR(40),
 confirmPass VARCHAR(40)
);
 
