INSERT INTO users VALUES(4, 'first_name', 'last_name', '1967-11-17', 'F', 'email', 'pass', 'confirmPass');
 
SELECT * FROM USERS;
 
SELECT * from USERS where user_id = 1;
 
SELECT *
FROM users
WHERE sex = 'M';
 
Delete from users where user_id = 51;
 
